{
  "name": "webpack-starter-template",
  "version": "1.0.0",
  "description": "A simple starter template for setting up a ECMAScript 6 project with barba.js and webpack.",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=development gulp --require @babel/register",
    "build": "NODE_ENV=production gulp build --require @babel/register",
    "prettier": "prettier --write src/",
    "lint:css": "stylelint \"**/*.{css,less,styl,scss,sass,sss,tsx}\" --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --ignore-path .gitignore --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.0.6",
    "clean-html": "^1.5.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.5",
    "dynamic-webpack-entries": "^1.0.1",
    "eslint": "^7.27.0",
    "file-include-webpack-plugin": "1.3.6",
    "husky": "^6.0.0",
    "jquery": "^3.6.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.2.1",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "slick-carousel": "1.8.1",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.1.2",
    "url-loader": "^4.1.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/register": "^7.13.16",
    "@gsap/business": "^3.7.0",
    "bootstrap": "4.6",
    "bootstrap-select": "^1.13.18",
    "browser-sync": "^2.26.14",
    "browserslist": "^4.16.6",
    "del": "^6.0.0",
    "gsap": "npm:@gsap/business@^3.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-file-include": "^2.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-version-number": "^0.2.4",
    "node": "^16.1.0",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "process": "^0.11.10",
    "reset-css": "^5.0.1",
    "webpack-dev-middleware": "^4.3.0"
  }
}
